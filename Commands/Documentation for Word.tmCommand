clhyper --path $SYMBOL | xargs -I % wget -q -O - % | awk '/&lt;HR&gt;/,/&lt;\/UL&gt;&lt;HR&gt;/' | sed 's/^&lt;HR&gt;//' | sed 's/&lt;HR&gt;$//'

html_footer</string>
	<key>fallbackInput</key>
	<string>word</string>
	<key>input</key>
	<string>selection</string>
	<key>keyEquivalent</key>
	<string>^h</string>
